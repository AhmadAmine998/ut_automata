{
  "name": "f1tenth-course",
  "version": "1.0.0",
  "description": "Server for remote web interface for F1/10 cars.",
  "main": "dist/index.js",
  "repository": "https://github.com/umass-amrl/f1tenth_course",
  "author": "Joydeep Biswas <joydeepb@cs.utexas.edu>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "clean": "rm -f dist/*",
    "serve": "NODE_ENV=production node dist/index.js",
    "client": "node dist/robot-client.js"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)test)\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "@types/compression": "^0.0.36",
    "@types/jpeg-js": "^0.3.0",
    "@types/node": "^10.0.0",
    "@types/sharp": "^0.17.10",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "http-post": "^0.1.1",
    "internal-ip": "^4.3.0",
    "jest": "^23.6.0",
    "jpeg-js": "^0.3.4",
    "roslib": "^1.0.1",
    "rosnodejs": "^3.0.0",
    "sharp": "^0.22.0",
    "ts-jest": "^23.1.4",
    "typescript": "^3.0.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.0"
  }
}
